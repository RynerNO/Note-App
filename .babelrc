{
    "presets": [

        "@babel/preset-env"

    ],
    "plugins": [
        ["module-resolver", {
            "root": ["./"], 
            "alias": {
                "@components": "./src/components",
                "@pages": "./src/pages",
                "@styles": "./src/styles"
            }}
        ],
        ["@babel/plugin-transform-runtime",
            {
              "regenerator": true
            }
          ]
    ]
}